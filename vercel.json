{
  "version": 2,
  "cleanUrls": true,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "redirects": [
    {
      "source": "/old-path",
      "destination": "/new-path",
      "permanent": true
    },
    {
      "source": "/another-old-path",
      "destination": "https://external-site.com",
      "permanent": false
    }
  ]
}
